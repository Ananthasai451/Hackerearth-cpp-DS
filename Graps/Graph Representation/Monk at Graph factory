
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,t,s,r;
    s=0;
    cin>>n;
    r=(n-1)*2;
    for(int i=0;i<n;i++)
    {
        cin>>t;
        if(t>0)
        {
            s=s+t;
        }
        else
        {
            break;
        }
    }
    //cout<<r;
    if(s==r)
    {

        cout<<"Yes";
    }
    else{
        cout<<"No";
    }
}
