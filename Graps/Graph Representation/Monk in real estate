
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int x,y,n;
    unordered_map<int,int> um;
    cin>>n;
    for(int j=0;j<n;j++){
    cin>>x>>y;
    um[x]++;
    um[y]++;
    }
    cout<<um.size()<<endl;
    }
}
