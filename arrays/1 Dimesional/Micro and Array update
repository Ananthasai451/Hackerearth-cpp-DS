
#include<iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int n,k,t;
        cin>>n>>k;
        int m=9999999;
        for(int j=0;j<n;j++)
        {
            cin>>t;
            if (t<m)
            {
                m=t;
            }
        }
        if((k-m)>0)
        {
            cout<<k-m<<endl;
        }
        else
        {
            cout<<0<<endl;
        }

    }
    return 0;
}
